                                    <xs:annotation>
                                       <xs:documentation>l'intervention est justifiée par une preuve interne</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                              </xs:restriction>
                           </xs:simpleType>
                           <xs:simpleType>
                              <xs:restriction base="xs:token">
                                 <xs:enumeration value="external">
                                    <xs:annotation>
                                       <xs:documentation>l'intervention est justifiée par une preuve externe</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                              </xs:restriction>
                           </xs:simpleType>
                           <xs:simpleType>
                              <xs:restriction base="xs:token">
                                 <xs:enumeration value="conjecture">
                                    <xs:annotation>
                                       <xs:documentation>l'intervention ou l'interprétation a été faite par le rédacteur, le catalogueur, ou le chercheur sur la base de leur expertise.</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                              </xs:restriction>
                           </xs:simpleType>
                           <xs:simpleType>
                              <xs:restriction base="xs:token">
                                 <xs:pattern value="[^\p{C}\p{Z}]+"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:union>
                     </xs:simpleType>
                  </xs:list>
               </xs:simpleType>
               <xs:minLength value="1"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
