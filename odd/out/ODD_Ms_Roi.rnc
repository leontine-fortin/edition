    list {
      xsd:anyURI { pattern = "\S+" }+
    }
  }?
w =
  
  ## (mot) représente un mot grammatical (pas nécessairement orthographique) [17.1. Linguistic Segment Categories 17.4.2. Lightweight Linguistic Annotation]
  element w {
    ((text
      | model.gLike
      | seg
      | gap
      | space
      | model.hiLike
      | model.milestoneLike
      | model.noteLike
      | choice
      | subst
      | model.pPart.transcriptional)+)
    >> s:pattern [
         id = "Manuscrit_Roi-w-noBlanksInW-constraint-assert-16"
         "\x{a}" ~
         "            "
         s:rule [
           context = "tei:w"
           "\x{a}" ~
           "               "
           s:assert [
             test =
               ".[not(child::text()[normalize-space(.) != '' and contains(., ' ')])]"
             "Les mots (w) ne doivent pas contenir de nœuds de textes\x{a}" ~
             "                                        avec des blancs (utiliser\x{a}" ~
             "                                    space)."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attribute.xmlid,
    att.global.attribute.n,
    att.global.attribute.xmllang,
    att.global.linking.attribute.corresp,
    att.global.linking.attribute.sameAs,
    att.global.analytic.attribute.ana,
    att.global.responsibility.attribute.cert,
    att.global.responsibility.attribute.resp,
    att.global.source.attribute.source,
    att.segLike.attributes,
    att.typed.attributes,
    att.linguistic.attributes,
    att.notated.attributes,
    
    ## (interprétation) indique comment l'élément en question a été rendu ou présenté dans le texte source
    attribute rend {
      list {
        (
         ## spécifie que deux mots sont unis dans la source (sauf cas d'élision)
         "aggl"
         | 
           ## spécifie que, pour deux mots unis dans la source, le premier donne lieu à une élision
           "elision"
         | 
           ## spécifie que, pour deux mots séparés dans la source, le premier donne lieu à une élision
           "elision-sans-aggl"
         | 
           ## spécifie qu'un article, agrégé au mot précédent dans la source, donne lieu à une élision
           "enclise")+
      }
    }?,
    empty
  }
c =
  
  ## (caractère) représente un caractère [17.1. Linguistic Segment Categories]
  element c {
    macro.xtext,
    att.global.facs.attributes,
    att.global.attributes,
