pc =
  
  ## (punctuation character) contient un caractère ou une chaîne de caractères considérés comme un signe de ponctuation unique. [17.1.2. Below the Word Level 17.4.2. Lightweight Linguistic Annotation]
  element pc {
    (text | model.gLike | c | model.pPart.edit)*,
    att.global.attributes,
    att.segLike.attributes,
    att.linguistic.attributes,
    
    ## caractérise l'élément en utilisant n'importe quel système ou typologie de classification approprié.
    attribute type {
      
      ## caractérise la ponctuation éditoriale
      "supplied"
      | 
        ## peut être utilisé pour distinguer la ponctuation originale
        "orig"
    },
    
    ##
    attribute subtype {
      
      ## caractérise un signe de ponctuation utilisé comme multiplicateur
      "multiplicateur"
      | 
        ## caractérise un signe de ponctuation utilisé comme séparateur dans une énumération
        "enumeration"
      | 
        ## caractérise un signe de ponctuation utilisé comme séparateur dans la mise en page (séparateur de colonne, etc.)
        "miseEnPage"
      | 
        ## caractérise un signe de ponctuation utilisé comme séparateur de colonne ou point final de vers
        "colSep"
      | 
        ##
        "enjambement"
      | 
        ##
        "corrMetrique"
      | 
        ##
        "juxtaposition"
      | 
        ## caractérise un signe de ponctuation annonçant un discours
        "discours"
    }?,
    empty
  }
