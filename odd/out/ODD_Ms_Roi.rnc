    att.dimensions.attributes,
    att.typed.attributes,
    empty
  }
abbr =
  
  ## (abréviation) contient une abréviation quelconque. [3.6.5. Abbreviations and Their Expansions]
  element abbr {
    macro.phraseSeq,
    att.global.attributes,
    
    ## (type) permet à l'encodeur de caractériser l'abréviation selon une typologie adéquate
    ## Sample values include: 1] suspension (suspension); 2] contraction (contraction); 3] brevigraph; 4] superscription (superscription); 5] acronym (acronym); 6] title (title); 7] organization (organization); 8] geographic (geographic)
    attribute type {
